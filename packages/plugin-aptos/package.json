{
  "name": "@elizaos/plugin-aptos",
  "version": "0.1.9",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.26.0",
    "@elizaos/core": "workspace:^",
    "@pinata/sdk": "^1.0.1",
    "agent-twitter-client": "^0.0.18",
    "axios": "^1.7.9",
    "bignumber.js": "9.1.2",
    "ethers": "^6.11.1",
    "langchain": "^0.1.9",
    "node-cache": "5.1.2",
    "pinata-web3": "^0.5.4",
    "tsup": "8.3.5",
    "vitest": "2.1.9"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "test": "vitest run",
    "lint": "biome lint .",
    "lint:fix": "biome check --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "ts-node": "^10.9.2",
    "ts-node-esm": "^0.0.6",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "form-data": "4.0.1",
    "whatwg-url": "7.1.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "APTOS_PRIVATE_KEY": {
        "type": "string",
        "minLength": 1,
        "description": "Aptos private key is required"
      },
      "APTOS_NETWORK": {
        "type": "string",
        "enum": [
          "mainnet",
          "testnet"
        ],
        "description": "Network to connect to (mainnet or testnet)"
      }
    }
  }
}